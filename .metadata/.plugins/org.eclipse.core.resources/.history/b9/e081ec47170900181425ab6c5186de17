package mypack.DeltaToFeature;

import org.eclipse.emf.ecore.EObject;
import org.eclipse.xtext.documentation.IEObjectDocumentationProvider;
import org.xtext.abs.Delta_decl;

class DeltaToFeature implements IResourceSetProvider {

	@Override
	public String getDocumentation(EObject o) {
		System.out.println("Yes I am runningg!!!!!");
		if (o instanceof Delta_decl) {
			return "This is a nice Greeting";
		}
		return null;
	}

}